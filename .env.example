# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# OpenAI Configuration (Optional - for Phase 3 AI insights)
VITE_OPENAI_API_KEY=sk-your-openai-key-here

# Environment
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_AI_INSIGHTS=false
VITE_ENABLE_FOCUS_ROOMS=false

# Blink.new Full-Stack Configuration
VITE_BLINK_DEPLOYMENT=false
VITE_NODE_ENV=development

# Blink.new Serverless Runtime
# Configure these when deploying to blink.new
# BLINK_PUBLIC_DIR=dist
# BLINK_FUNCTION_REGIONS=global

# API & Service Configuration
VITE_API_URL=http://localhost:5173/api
VITE_WS_URL=ws://localhost:5173

# Session & Security
VITE_SESSION_TIMEOUT=3600
VITE_SECURE_COOKIES=false

# Feature Flags for Blink.new
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_OFFLINE_MODE=true
